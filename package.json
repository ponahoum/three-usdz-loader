    "eslint": "^8.17.0",
    "prettier": "^2.6.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.3",
    "@types/three": "^0.161.0"
  },
  "dependencies": {
    "three": "^0.161.0"
  },
  "peerDependencies": {
    "three": "^0.161.0"
  },
  "repository": {
